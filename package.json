{
  "name": "eden-electron",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm:start:server\" \"npm:start:craco\"",
    "run-electron": "concurrently \"BROWSER=none npm start\"  \"wait-on http://localhost:3000 && electron-forge start\"",
    "build": "craco build",
    "test": "craco test",
    "postcss:examiner": "npx postcss src/examiner/App.css -o src/examiner/tailwind.css",
    "postcss:examinee": "npx postcss src/examinee/App.css -o src/examinee/tailwind.css",
    "start:server": "npx nodemon --trace-warnings src/examiner/processes/server.js",
    "start:craco": "craco start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "concurrently": "^6.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "fast-csv": "^4.3.6",
    "multer": "^1.4.2",
    "postcss-cli": "^7.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fetch-hook": "^1.8.5",
    "react-hook-form": "^7.3.4",
    "react-icons": "^4.2.0",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.1",
    "@types/sequelize": "^4.28.9",
    "autoprefixer": "^9.8.6",
    "cors": "^2.8.5",
    "electron": "^13.6.6",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-tailwind": "^0.2.1",
    "nodemon": "^2.0.7",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s3818311/eden-electron.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/s3818311/eden-electron/issues"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "eden_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
